#include <iostream>
using namespace std;

int main()
{
	int* arr = new int[100];
	int* arr1 = new int[50];

	for (int i = 0; i < 100; i++)
		arr[i] = i;
	for (int i = 0; i < 50; i++)
		arr1[i] = i;

	swap(arr[0], arr1[49]);

	cout << "arr[100]" <<  arr[0] << endl;
	cout << "arr1[50]" <<  arr1[49] << endl;

	delete[] arr;
	delete[] arr1;

	return 0;
}
