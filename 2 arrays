#include <iostream>
using namespace std;

int main()
{
    int arr100[100];
    int arr50[50];
    for (int i = 0; i < 100; i++)
        arr100[i] = i;
    for (int i = 0; i < 50; i++)
        arr50[i] = i;

    swap(arr100[0], arr50[49]);

    cout << "arr100[0]" << arr100[0] << endl;
    cout << "arr50[50]" << arr50[49] << endl;
}
